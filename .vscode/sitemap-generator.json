{
  "public/sitemap.xml": {
    "Protocol": "https",
    "DomainName": "virtual-periodictable.web.app",
    "Root": "./public/",
    "IncludeExt": [
      ".html",
      ".php"
    ],
    "Exclude": [
      "404.html",
      "google85bf64fc444d13bb.html",
      "./public/very_short_descriptions/**"
    ],
    "TagsToInclude": [
      "priority",
      "lastmod"
    ],
    "DefaultChangeFrequency": "daily",
    "TabCharacters": "\t",
    "bIncludeWWW": true,
    "bRemoveFileExtentions": false,
    "bUseTrailingSlash": false,
    "bAutomaticallyUpdateSitemap": true,
    "bMinimized": false
  }
}