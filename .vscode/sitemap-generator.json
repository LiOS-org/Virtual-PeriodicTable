{
  "public/sitemap.xml": {
    "Protocol": "https",
    "DomainName": "virtual-periodictable.web.app",
    "Root": "./public/",
    "IncludeExt": [
      ".html",
      ".php"
    ],
    "Exclude": [],
    "TagsToInclude": [
      "priority",
      "lastmod"
    ],
    "DefaultChangeFrequency": "daily",
    "TabCharacters": "\t",
    "bIncludeWWW": true,
    "bRemoveFileExtentions": false,
